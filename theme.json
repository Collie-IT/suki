{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": true,
			"link": true,
			"duotone": [],
			"customDuotone": false,
			"gradients": [],
			"customGradient": false
		},
		"typography": {
			"fontFamilies": [],
			"fontSizes": [
				{
					"name": "Small",
					"slug": "small",
					"size": "0.9rem"
				},
				{
					"name": "Normal",
					"slug": "normal",
					"size": "1rem"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "1.25rem"
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "1.5rem"
				},
				{
					"name": "Extra Large",
					"slug": "extra-large",
					"size": "1.8rem"
				},
				{
					"name": "Huge",
					"slug": "huge",
					"size": "2.4rem"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"lineHeight": true,
			"textDecoration": true,
            "textTransform": true,
			"customFontSize": true,
			"dropCap": true
		},
		"layout": {
			"contentSize": "var(--content-width--narrow)",
			"wideSize": "var(--content-width--wide)"
		},
		"spacing": {
			"blockGap": true,
			"padding": true,
			"margin": true,
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"custom": {
			"line-height": {
				"small": "1.7",
				"normal": "1.7",
				"medium": "1.6",
				"large": "1.5",
				"extra-large": "1.4",
				"huge": "1.3"
			}
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "var(--block-spacing)"
		}
	},
	"customTemplates": [],
	"templateParts": []
}